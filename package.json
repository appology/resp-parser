{
  "name": "resp-parser",
  "version": "0.0.8",
  "description": "A grammar and parser for the RESP protocol.",
  "main": "index.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,ne",
      "ignore": "src/nearley/grammar.js",
      "quiet": false
    },
    "nearley": {
      "patterns": [
        "src/nearley"
      ],
      "extensions": "ne",
      "quiet": false
    }
  },
  "scripts": {
    "nearley": "node scripts/nearley.js",
    "babel": "node scripts/babel.js",
    "build": "node scripts/build.js",
    "postinstall": "npm run build",
    "test": "node scripts/test.js",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appology/resp-parser.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/appology/resp-parser/issues"
  },
  "homepage": "https://github.com/appology/resp-parser#readme",
  "dependencies": {
    "nearley": "^2.11.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "npm-watch": "^0.2.0",
    "prettyjson": "^1.2.1",
    "shelljs": "^0.7.8"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
